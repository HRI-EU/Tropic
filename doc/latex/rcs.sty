% Eigene caption Umgebung. Ist die Bildunterschrift breiter als \lensoll
% erfolgt eine mehrzeilige, eingerueckte Darstellung. Ueber \lensoll
% kann prinzipiell die Schriftbreite eingestellt werden.

\newlength{\lenlabel}
\newlength{\lensoll}
\newcommand{\mycaption}[1]{
  \refstepcounter{figure}
  \settowidth{\lenlabel}{\small #1}
  \setlength{\lensoll}{100mm}
  \ifdim \lenlabel < \lensoll
  \begin{center}
    {\small \bf Bild \thefigure:}
    \parbox[t]{\lenlabel}{\small #1}
  \end{center}
  \else
  \begin{center}
    {\small \bf Bild \thefigure:}
    \parbox[t]{\lensoll}{\small #1}
  \end{center}
  \fi
  }

\newcommand{\mytablecaption}[1]{
  \refstepcounter{table}
  \settowidth{\lenlabel}{\small #1}
  \setlength{\lensoll}{100mm}
  \ifdim \lenlabel < \lensoll
  \begin{center}
    {\small \bf Tabelle \thetable:}
    \parbox[t]{\lenlabel}{\small #1}
  \end{center}
  \else
  \begin{center}
    {\small \bf Tabelle \thetable:}
    \parbox[t]{\lensoll}{\small #1}
  \end{center}
  \fi
  }

\newcommand{\mysidewaystablecaption}[1]{
  \refstepcounter{table}
  \settowidth{\lenlabel}{\small #1}
  \setlength{\lensoll}{160mm}
  \ifdim \lenlabel < \lensoll
  \begin{center}
    {\small \bf Tabelle \thetable:}
    \parbox[t]{\lenlabel}{\small #1}
  \end{center}
  \else
  \begin{center}
    {\small \bf Tabelle \thetable:}
    \parbox[t]{\lensoll}{\small #1}
  \end{center}
  \fi
  }

